#! /bin/csh
# *Cassiopee* Shadow Environment

#---------------------------- User Data --------------------------------------
# Using MPI?
setenv MPI false
# Using debug mode?
setenv DBG false
#-----------------------------------------------------------------------------

# Full version
setenv CASSIOPEE_ORIG $CASSIOPEE
setenv CASSIOPEE /home/cwervaec/Cassiopee
if (-e $CASSIOPEE/Apps/Modules/Envs/env_Cassiopee_local) then
    source $CASSIOPEE/Apps/Modules/Envs/env_Cassiopee_local
else
    source $CASSIOPEE/Dist/env_Cassiopee_local
endif

# Local version
setenv CASSIOPEE $CASSIOPEE_ORIG
if (-e $CASSIOPEE/Apps/Modules/Envs/env_Cassiopee_local) then
    source $CASSIOPEE/Apps/Modules/Envs/env_Cassiopee_local
else
    source $CASSIOPEE/Dist/env_Cassiopee_local
endif
