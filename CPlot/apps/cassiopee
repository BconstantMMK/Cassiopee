#! /bin/sh
if [ "$PYTHONEXE" != "" ]; then
    alias python=$PYTHONEXE
fi
if [ "$ELSAPROD" = "vis_r8" -o "$ELSAPROD" = "flv_r8" -o "$ELSAPROD" = "spiro_r8" ]; then
	if test -e "$CASSIOPEE/Dist/bin/$ELSAPROD/tkCassiopee.py"
	then
    	vglrun python "$CASSIOPEE/Dist/bin/$ELSAPROD/tkCassiopee.py" "$@"
	else
    	vglrun python "$CASSIOPEE/Dist/bin/$ELSAPROD/tkCassiopee.pyc" "$@"
	fi
else
	if test -e "$CASSIOPEE/Dist/bin/$ELSAPROD/tkCassiopee.py"
	then
    	python "$CASSIOPEE/Dist/bin/$ELSAPROD/tkCassiopee.py" "$@"
	else
    	python "$CASSIOPEE/Dist/bin/$ELSAPROD/tkCassiopee.pyc" "$@"
	fi
fi
