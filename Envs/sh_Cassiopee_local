    module load CFD/4.0-intel-17.impi
    #module load intel/17.0.4 impi/17 python/2.7.8-intel-17.impi
    #module load gcc/5.2 intel/17.0.4 impi/17 python/2.7.8-intel-17.impi
    #module load gcc/5.2 intel/17.0.4 impi/17 python/2.7.13-intel-17
    # HDF para (deja dans le socle)
    #module load hdf5/1.8.17-intel-17.impi

    # Chemin de l'executable python
    export PATH=$PATH
    export PATH=$PATH:$CASSIOPEE/Dist/bin
    export PATH=$CASSIOPEE/Dist/bin/$ELSAPROD:$CASSIOPEE/Dist/bin/$ELSAPROD/bin:$PATH
    export LD_LIBRARY_PATHL=$CASSIOPEE/Kernel/lib/"$ELSAPROD":$CASSIOPEE/Dist/bin/"$ELSAPROD":$CASSIOPEE/Dist/bin/"$ELSAPROD"/lib
    export PYTHONPATHL=$CASSIOPEE/Dist/lib/py:$CASSIOPEE/Dist/bin/"$ELSAPROD":$CASSIOPEE/Dist/bin/"$ELSAPROD":$CASSIOPEE/Dist/bin/"$ELSAPROD"/lib/python2.7/site-packages

elif [ "$MAC" == "sator_sky" ]; then
#----------------------------- sator skylake ---------------------------------------
    export ELSAPROD=sat_sky_r8
    echo "elsaProd=" $ELSAPROD
    # Nbre de threads
