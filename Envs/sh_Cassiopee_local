    # LD_PRELOAD libirc for Intel Python
    #export LD_PRELOAD=/stck5/nalferez/intel/2018update2/PythonIntel/intelpython2/lib/libirc.so:$LD_PRELOAD

elif [ "$MAC" == "westri" ]; then
#----------------------------- westri (KNL) -----------------------------------------
