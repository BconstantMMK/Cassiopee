#! /bin/csh
# *Cassiopee* Compilation Environment

#---------------------------- User Data --------------------------------------
# Using MPI?
setenv MPI true
# Using debug mode?
setenv DBG true
#-----------------------------------------------------------------------------

if (-e $CASSIOPEE/Apps/Modules/Envs/env_Cassiopee_local) then
    source $CASSIOPEE/Apps/Modules/Envs/env_Cassiopee_local
else
    source $CASSIOPEE/Dist/env_Cassiopee_local
endif
